<h1>Accounts</h1>
<%= button_to "Log Out", destroy_user_session_path, method: :delete, class:'waves-effect waves-light btn' %>
<h2>Welcome <%= current_user.first_name %></h2>
<hr />
<%= link_to "Add Account", new_account_path %>
<h3>Your Accounts</h3>

<div class="row">
  <% @accounts.each do |account| %>
   <%= render "account_card", account: account%>
  <% end %>
</div>





  
  
